<h1>Welcome#index</h1>
<p>Hi Everyone</p>

<div id="room-id" data-room-id="<%= @room.try(:id) %>"></div>
<div id="user-id" data-user-id="<%= current_user.id %>"></div>

<div class="container-fluid">
      <div class="row">
        <div class="col-3 rooms-sidebar">
          <div class="card no-outline">
            <div class="card-body">
              <small>Logged in as</small><br>
              <b><%= current_user.username %><br></b>
              <br><b>
                <%= link_to "Logout", destroy_user_session_path, method: :delete, class:"button is-info" %>
              </b>
            </div>
          </div>
              <br>
              [Online User List] <br>
              ===============
              <% @users = User.all %>
              <% @users.each do |user| %>
                <%= user.username %><br/>
              <% end %>
              ===============
              <script type="text/template" id="search-item-tmpl">
                {{=username}}
                Hello!
              </script>

              <div id="search-app">
              <label>username: <input id="username-input" type="text"></label>
              <button class="search">Search</button>
                  <div class="search-list">
              </div>
              ================

        </div>
    </div>
</div>